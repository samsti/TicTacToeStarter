<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easv.tictactoe.gui.controller.NameWindowController">
   <children>
      <Label layoutX="35.0" layoutY="14.0" text="Welcome to the TicTacToe game">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="user1" layoutX="15.0" layoutY="93.0" />
      <TextField fx:id="user2" layoutX="184.0" layoutY="93.0" />
      <Label layoutX="73.0" layoutY="76.0" text="Player 1" />
      <Label layoutX="243.0" layoutY="76.0" text="Player 2" />
      <Label layoutX="122.0" layoutY="49.0" text="Enter your names:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="playButton" layoutX="118.0" layoutY="149.0" mnemonicParsing="false" onAction="#clickOpenWindow" prefHeight="26.0" prefWidth="112.0" text="Play" />
   </children>
</AnchorPane>
